{
  "name": "payline-sdk",
  "version": "1.0.0",
  "description": "Payline Typescript SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Chez Nestor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chez-nestor/payline-sdk.git"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "lint": "./node_modules/.bin/eslint \"./src/**\" --ext .ts",
    "build": "npm run lint && tsc -p . && npm run copy-wsdl",
    "link-all": "npm run build && npm link",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "copy-wsdl": "copyfiles -u 1 './src/config/wsdls/**' ./dist"
  },
  "keywords": [
    "payline",
    "nodejs",
    "typescript",
    "payment",
    "sdk"
  ],
  "files": [
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "crypto-random-string": "3.2.0",
    "moment-timezone": "0.5.28",
    "soap": "^0.36.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/mocha": "2.2.48",
    "@types/moment-timezone": "0.5.12",
    "@types/node": "13.11.1",
    "@typescript-eslint/eslint-plugin": "2.22.0",
    "@typescript-eslint/parser": "2.22.0",
    "chai": "4.2.0",
    "eslint": "6.8.0",
    "copyfiles": "2.2.0",
    "mocha": "6.1.4",
    "reflect-metadata": "0.1.13",
    "ts-node": "8.8.1",
    "typescript": "3.8.3"
  },
  "private": false
}
